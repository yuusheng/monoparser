#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yuusheng/project/custom-parsers/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yuusheng/project/custom-parsers/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/registry.npmmirror.com+marked@4.1.0/node_modules/marked/bin/marked.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/registry.npmmirror.com+marked@4.1.0/node_modules/marked/bin/marked.js" "$@"
fi
