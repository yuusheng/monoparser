{
  "name": "@monoparser/md-parser",
  "version": "0.1.1",
  "discription": "A simple markdown parser base on marked",
  "author": "yuusheng <contact@yuusheng.cn>",
  "homepage": "https://github.com/yuusheng/monoparser",
  "repository": "https://github.com/yuusheng/monoparser/tree/master/packages/md-parser",
  "keywords": [
    "markdown",
    "marked"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./bodyParser": {
      "types": "./dist/bodyParser.d.ts",
      "require": "./dist/bodyParser.cjs",
      "import": "./dist/bodyParser.mjs"
    },
    "./headerParser": {
      "types": "./dist/headerParser.d.ts",
      "require": "./dist/headerParser.cjs",
      "import": "./dist/headerParser.mjs"
    },
    "./parser": {
      "types": "./dist/parser.d.ts",
      "require": "./dist/parser.cjs",
      "import": "./dist/parser.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "**/*.css"
  ],
  "scripts": {
    "test": "vitest",
    "build": "unbuild"
  },
  "dependencies": {
    "marked": "^4.1.0"
  },
  "devDependencies": {
    "@types/marked": "^4.0.7"
  }
}
